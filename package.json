	"dependencies": {
		"@floating-ui/dom": "^1.0.0",
		"@lezer/cpp": "^1.0.0",
		"@lezer/css": "^1.0.1",
		"@lezer/highlight": "^1.1.2",
		"@lezer/html": "^1.0.1",
		"@lezer/javascript": "^1.0.2",
		"@lezer/json": "^1.0.0",
		"@lezer/python": "^1.1.1",
		"@lezer/rust": "^1.0.0",
		"@mdi/js": "^7.1.96",
		"@replit/codemirror-lang-svelte": "^6.0.0",
		"uid": "^1.0.0"
	},
	"devDependencies": {
		"@kwangure/eslint-config-svelte": "^0.2.9",
		"@kwangure/svelte-docs": "^0.5.0",
		"@playwright/test": "^1.30.0",
		"@sveltejs/adapter-vercel": "^1.0.1",
		"@sveltejs/kit": "^1.15.9",
		"@sveltejs/package": "^2.0.2",
		"eslint": "^8.12.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"husky": "^8.0.3",
		"micromatch": "^4.0.4",
		"playwright": "^1.27.1",
		"publint": "^0.1.11",
		"release-it": "^15.5.0",
		"svelte": "^3.48.0",
		"svelte-check": "^2.7.1",
		"svelte2tsx": "^0.6.11",
		"sveltekit-markdoc": "^0.0.11",
		"typescript": "^5.0.4",
		"vite": "^4.0.4",
		"vite-plugin-inspect": "^0.7.12",
		"vitest": "^0.24.3",
		"wireit": "^0.9.5"
	},
	"peerDependencies": {
		"svelte": "^3.48.0"
	},
	"keywords": [
		"svelte",
		"sveltejs",
		"component",
		"components",
		"button",
		"code",
		"dropdown",
		"icon",
		"input",
		"list",
		"modal",
		"navbar",
		"notification",
		"select",
		"sidebar",
		"steps",
		"tooltip"
	],
	"type": "module",
	"wireit": {
		"build": {
			"command": "vite build",
			"files": [
				"src/**/*.{css,js,ts,svelte,md}",
				"static/**",
				"svelte.config.js",
				"vite.config.js"
			],
			"output": [
				"build/**"
			]
		},
		"check": {
			"command": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
			"files": [
				"{src,test}/**/*.{js,ts,svelte}",
				"tsconfig.json"
			],
			"output": []
		},
		"format": {
			"command": "eslint \"{src,test}/**/*.{js,svelte}\" --color --cache --fix",
			"files": [
				"{src,test}/**/*.{js,ts,svelte}",
				".eslintignore",
				".eslintrc.cjs"
			],
			"output": []
		},
		"lint": {
			"command": "eslint \"{src,test}/**/*.{js,svelte}\" --color --cache",
			"files": [
				"{src,test}/**/*.{js,ts,svelte}",
				".eslintignore",
				".eslintrc.cjs"
			],
			"output": []
		},
		"package": {
			"command": "svelte-package",
			"files": [
				"src/lib/**/*.{js,css,ts,svelte}"
			],
			"output": [
				"dist/**"
			]
		},
		"pack": {
			"command": "npm pack",
			"dependencies": [
				"publint"
			],
			"files": [
				"dist/**",
				"!**/*.test.*",
				"!**/*.spec.*",
				"LICENSE",
				"README.md"
			],
			"output": [
				"kwangure-strawberry*.tgz"
			]
		},
		"pre-commit": {
			"dependencies": [
				"check",
				"format",
				"publint"
			]
		},
		"publint": {
			"command": "publint",
			"dependencies": [
				"package"
			],
			"files": [
				"dist/**",
				"package.json"
			],
			"output": []
		},
		"test": {
			"dependencies": [
				"test:vitest"
			]
		},
		"test:playwright": {
			"command": "playwright test"
		},
		"test:vitest": {
			"command": "vitest run"
		}
	},
	"files": [
		"dist",
		"!**/*.test.*",
		"!**/*.spec.*"
	]
}
