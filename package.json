			"default": "./dist/default/dialog/index.js"
		},
		"./default/dialog/layout.css": "./dist/default/dialog/layout.css",
		"./default/dialog/light.css": "./dist/default/dialog/light.css",
		"./default/dropdown/dark.css": "./dist/default/dropdown/dark.css",
		"./default/dropdown/index.css": "./dist/default/dropdown/index.css",
