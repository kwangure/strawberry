{
	"name": "@kwangure/strawberry",
	"version": "0.57.5",
	"description": "A set of svelte components",
	"scripts": {
		"build": "vite build",
		"check": "wireit",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"dev": "vite dev",
		"lint": "wireit",
		"format": "wireit",
		"package": "svelte-package",
		"pre-commit": "wireit",
		"prepare": "husky install",
		"preview": "vite preview",
		"release": "release-it",
		"test": "wireit"
	},
	"exports": {
		".": "./"
	},
	"dependencies": {
		"@floating-ui/dom": "^1.0.0",
		"@lezer/cpp": "^1.0.0",
		"@lezer/css": "^1.0.1",
		"@lezer/highlight": "^1.1.2",
		"@lezer/html": "^1.0.1",
		"@lezer/javascript": "^1.0.2",
		"@lezer/json": "^1.0.0",
		"@lezer/python": "^1.1.1",
		"@lezer/rust": "^1.0.0",
		"@mdi/js": "^7.1.96",
		"@replit/codemirror-lang-svelte": "^6.0.0",
		"uid": "^1.0.0"
	},
	"devDependencies": {
		"@kwangure/eslint-config-svelte": "^0.2.5",
		"@kwangure/svelte-docs": "^0.5.0",
		"@playwright/test": "^1.30.0",
		"@sveltejs/adapter-vercel": "^1.0.1",
		"@sveltejs/kit": "^1.0.7",
		"@sveltejs/package": "^1.0.0-next.6",
		"eslint": "^8.12.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"husky": "^8.0.3",
		"micromatch": "^4.0.4",
		"playwright": "^1.27.1",
		"release-it": "^15.5.0",
		"svelte": "^3.48.0",
		"svelte-check": "^2.7.1",
		"svelte2tsx": "^0.4.2",
		"sveltekit-markdoc": "^0.0.9",
		"typescript": "^4.7.4",
		"vite": "^4.0.4",
		"vite-plugin-inspect": "^0.7.12",
		"vitest": "^0.24.3",
		"wireit": "^0.9.5"
	},
	"peerDependencies": {
		"svelte": "^3.48.0"
	},
	"keywords": [
		"svelte",
		"sveltejs",
		"component",
		"components",
		"button",
		"code",
		"dropdown",
		"icon",
		"input",
		"list",
		"modal",
		"navbar",
		"notification",
		"select",
		"sidebar",
		"steps",
		"tooltip"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/kwangure/strawberry.git"
	},
	"author": "Kafungo Wangure",
	"bugs": {
		"url": "https://github.com/kwangure/strawberry/issues"
	},
	"homepage": "https://kwangure.github.io/strawberry",
	"type": "module",
	"wireit": {
		"check": {
			"command": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json"
		},
		"format": {
			"command": "eslint \"**/*.{js,svelte}\" --color --cache --fix --cache-location .eslintcache",
			"files": [
				"\"**/*.{js,svelte,ts}\"",
				".eslintignore",
				".eslintrc.cjs"
			]
		},
		"lint": {
			"command": "eslint \"**/*.{js,svelte}\" --color --cache --cache-location .eslintcache",
			"files": [
				"\"**/*.{js,svelte,ts}\"",
				".eslintignore",
				".eslintrc.cjs"
			]
		},
		"pre-commit": {
			"dependencies": [
				"check",
				"format"
			]
		},
		"test": {
			"dependencies": [
				"test:vitest"
			]
		},
		"test:playwright": {
			"command": "playwright test"
		},
		"test:vitest": {
			"command": "vitest run"
		}
	}
}
