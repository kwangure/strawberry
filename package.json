{
	"name": "@kwangure/strawberry",
	"version": "0.57.5",
	"description": "A set of svelte components",
	"scripts": {
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"clear": "rm -rf ./kwangure-strawberry-*.tgz",
		"dev": "vite dev",
		"lint": "wireit",
		"format": "wireit",
		"prepack": "wireit",
		"package": "wireit",
		"pre-commit": "wireit",
		"prepare": "husky install",
		"preview": "vite preview",
		"publint": "publint",
		"release": "release-it",
		"test": "wireit"
	},
	"exports": {
		"./actions/autosize": {
			"types": "./dist/actions/autosize.d.ts",
			"default": "./dist/actions/autosize.js"
		},
		"./actions/validate": {
			"types": "./dist/actions/validate.d.ts",
			"default": "./dist/actions/validate"
		},
		"./components/dialog": {
			"types": "./dist/components/dialog/index.d.ts",
			"svelte": "./dist/components/dialog/index.js"
		},
		"./components/dropdown": {
			"types": "./dist/components/dropdown/index.d.ts",
			"svelte": "./dist/components/dropdown/index.js"
		},
		"./components/element": {
			"types": "./dist/components/element/index.d.ts",
			"svelte": "./dist/components/element/index.js"
		},
		"./components/input/container": {
			"types": "./dist/components/input/container/index.d.ts",
			"svelte": "./dist/components/input/container/index.js"
		},
		"./components/input/number": {
			"types": "./dist/components/input/number/index.d.ts",
			"svelte": "./dist/components/input/number/index.js"
		},
		"./components/sidebar": {
			"types": "./dist/components/sidebar/index.d.ts",
			"svelte": "./dist/components/sidebar/index.js"
		},
		"./components/switch": {
			"types": "./dist/components/switch/index.d.ts",
			"svelte": "./dist/components/switch/index.js"
		},
		"./components/tooltip": {
			"types": "./dist/components/tooltip/index.d.ts",
			"svelte": "./dist/components/tooltip/index.js"
		},
		"./css/global.css": "./dist/css/global.css",
		"./css/reset.css": "./dist/css/reset.css",
		"./css/button.css": "./dist/css/button.css",
		"./default/dialog/dark.css": "./dist/default/dialog/dark.css",
		"./default/dialog/index.css": "./dist/default/dialog/index.css",
		"./default/dialog": {
			"types": "./dist/default/dialog/index.d.ts",
			"svelte": "./dist/default/dialog/index.js"
		},
		"./default/dialog/layout.css": "./dist/default/dialog/layout.css",
		"./default/dialog/light.css": "./dist/default/dialog/light.css",
		"./default/dropdown/dark.css": "./dist/default/dropdown/dark.css",
		"./default/dropdown/index.css": "./dist/default/dropdown/index.css",
		"./default/dropdown": {
			"types": "./dist/default/dropdown/index.d.ts",
			"svelte": "./dist/default/dropdown/index.js"
		},
		"./default/dropdown/layout.css": "./dist/default/dropdown/layout.css",
		"./default/dropdown/light.css": "./dist/default/dropdown/light.css",
		"./default/element": {
			"types": "./dist/default/element/index.d.ts",
			"svelte": "./dist/default/element/index.js"
		},
		"./default/input/container/dark.css": "./dist/default/input/container/dark.css",
		"./default/input/container/index.css": "./dist/default/input/container/index.css",
		"./default/input/container": {
			"types": "./dist/default/input/container/index.d.ts",
			"svelte": "./dist/default/input/container/index.js"
		},
		"./default/input/container/layout.css": "./dist/default/input/container/layout.css",
		"./default/input/container/light.css": "./dist/default/input/container/light.css",
		"./default/input/number/dark.css": "./dist/default/input/number/dark.css",
		"./default/input/number/index.css": "./dist/default/input/number/index.css",
		"./default/input/number": {
			"types": "./dist/default/input/number/index.d.ts",
			"svelte": "./dist/default/input/number/index.js"
		},
		"./default/input/number/layout.css": "./dist/default/input/number/layout.css",
		"./default/input/number/light.css": "./dist/default/input/number/light.css",
		"./default/item": {
			"types": "./dist/default/item/index.d.ts",
			"svelte": "./dist/default/item/index.js"
		},
		"./default/item/style.css": "./dist/default/item/style.css",
		"./default/navbar/dark.css": "./dist/default/navbar/dark.css",
		"./default/navbar/index.css": "./dist/default/navbar/index.css",
		"./default/navbar": {
			"types": "./dist/default/navbar/index.d.ts",
			"svelte": "./dist/default/navbar/index.js"
		},
		"./default/navbar/light.css": "./dist/default/navbar/light.css",
		"./default/sidebar": {
			"types": "./dist/default/sidebar/index.d.ts",
			"svelte": "./dist/default/sidebar/index.js"
		},
		"./default/sidebar/style.css": "./dist/default/sidebar/style.css",
		"./default/switch/dark.css": "./dist/default/switch/dark.css",
		"./default/switch/index.css": "./dist/default/switch/index.css",
		"./default/switch": {
			"types": "./dist/default/switch/index.d.ts",
			"svelte": "./dist/default/switch/index.js"
		},
		"./default/switch/layout.css": "./dist/default/switch/layout.css",
		"./default/switch/light.css": "./dist/default/switch/light.css",
		"./default/tooltip/dark.css": "./dist/default/tooltip/dark.css",
		"./default/tooltip/index.css": "./dist/default/tooltip/index.css",
		"./default/tooltip": {
			"types": "./dist/default/tooltip/index.d.ts",
			"svelte": "./dist/default/tooltip/index.js"
		},
		"./default/tooltip/layout.css": "./dist/default/tooltip/layout.css",
		"./default/tooltip/light.css": "./dist/default/tooltip/light.css",
		"./utils/events": {
			"types": "./dist/utils/events.d.ts",
			"svelte": "./dist/utils/events.js"
		}
	},
	"dependencies": {
		"@floating-ui/dom": "^1.0.0",
		"@lezer/cpp": "^1.0.0",
		"@lezer/css": "^1.0.1",
		"@lezer/highlight": "^1.1.2",
		"@lezer/html": "^1.0.1",
		"@lezer/javascript": "^1.0.2",
		"@lezer/json": "^1.0.0",
		"@lezer/python": "^1.1.1",
		"@lezer/rust": "^1.0.0",
		"@mdi/js": "^7.1.96",
		"@replit/codemirror-lang-svelte": "^6.0.0",
		"uid": "^1.0.0"
	},
	"devDependencies": {
		"@kwangure/eslint-config-svelte": "^0.2.5",
		"@kwangure/svelte-docs": "^0.5.0",
		"@playwright/test": "^1.30.0",
		"@sveltejs/adapter-vercel": "^1.0.1",
		"@sveltejs/kit": "^1.15.9",
		"@sveltejs/package": "^2.0.2",
		"eslint": "^8.12.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"husky": "^8.0.3",
		"micromatch": "^4.0.4",
		"playwright": "^1.27.1",
		"publint": "^0.1.11",
		"release-it": "^15.5.0",
		"svelte": "^3.48.0",
		"svelte-check": "^2.7.1",
		"svelte2tsx": "^0.6.11",
		"sveltekit-markdoc": "^0.0.9",
		"typescript": "^5.0.4",
		"vite": "^4.0.4",
		"vite-plugin-inspect": "^0.7.12",
		"vitest": "^0.24.3",
		"wireit": "^0.9.5"
	},
	"peerDependencies": {
		"svelte": "^3.48.0"
	},
	"keywords": [
		"svelte",
		"sveltejs",
		"component",
		"components",
		"button",
		"code",
		"dropdown",
		"icon",
		"input",
		"list",
		"modal",
		"navbar",
		"notification",
		"select",
		"sidebar",
		"steps",
		"tooltip"
	],
	"type": "module",
	"wireit": {
		"format": {
			"command": "eslint src/**/*.js test/**/*.js --color --cache --fix",
			"files": [
				"src/**/*.{js,ts}",
				"tests/**/*.{js,ts}",
				".eslintignore",
				".eslintrc.cjs"
			],
			"output": []
		},
		"lint": {
			"command": "eslint src/**/*.js test/**/*.js --color --cache",
			"files": [
				"src/**/*.{js,ts}",
				"tests/**/*.{js,ts}",
				".eslintignore",
				".eslintrc.cjs"
			],
			"output": []
		},
		"package": {
			"command": "svelte-package",
            "files": [
                "src/lib/*.{js,ts,svelte}"
            ],
            "output": [
                "dist/**"
            ]
		},
		"prepack": {
            "dependencies": [
                "clear",
                "package"
            ]
        },
		"pre-commit": {
			"dependencies": [
				"check",
				"format",
				"publint"
			]
		},
		"test": {
			"dependencies": [
				"test:vitest"
			]
		},
		"test:playwright": {
			"command": "playwright test"
		},
		"test:vitest": {
			"command": "vitest run"
		}
	},
	"files": [
		"dist",
		"!**/*.test.*",
		"!**/*.spec.*"
	]
}
