		"lint": "wireit",
		"format": "wireit",
		"pack": "wireit",
		"package": "wireit",
		"pre-commit": "wireit",
		"prepare": "husky install",
		"preview": "vite preview",
		"publint": "wireit",
		"release": "release-it",
		"test": "wireit"
